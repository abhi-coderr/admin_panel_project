<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">

        <shape>
            <solid android:color="@color/start_color" />
            <stroke android:width="@dimen/_1sdp" android:color="@color/start_color" />
            <corners android:radius="@dimen/_20sdp"/>
        </shape>

    </item>

    <item>

        <shape>
            <stroke android:width="@dimen/_1sdp" android:color="@color/white" />
            <corners android:radius="@dimen/_20sdp"/>
        </shape>

    </item>



</selector>